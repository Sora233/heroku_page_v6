<!DOCTYPE html>
<html>
<head>
  <title>HerokuPageV6</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= action_cable_meta_tag %>

  <%= favicon_pack_tag 'star.ico' %>

  <%= stylesheet_packs_with_chunks_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_packs_with_chunks_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<div class="container navbar-container">
  <%= render "shared/navbar" %>
</div>
<div class="container top-container">
  <div class="row">
    <div class="col-md-10 col-xs-12 col-md-12 col-md-offset-1">
      <%= render 'shared/alert' %>
      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
